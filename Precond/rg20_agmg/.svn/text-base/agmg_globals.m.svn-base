
%  available options for ktype are
% 'CG'     - flexible conjugate gradient
% 'GMRES'  - generalized minimal residual
% ktype = 'GMRES';
 ktype = 'CG';


% available smoothers
% 'J'      - Gauss Jacobi
% 'DJ'     - damped Jacobi
% 'symDJ'  - symmetric damped Jacobi
% 'GS'     - Gauss Seidel
% 'symGS'  - symmetric Gauss Seidel
% 'Cheb'   - Chebyshev polynomial smoothing
% smoother_type = 'J';
 smoother_type = 'DJ';
% smoother_type = 'symDJ';
% smoother_type = 'symGS';
% smoother_type = 'Cheb';


chebyshev_order ...
    = 5;

coarse_size = 10;

% strong threshold
threshold = 0.5;